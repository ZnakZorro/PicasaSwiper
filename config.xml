<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.phonegap.picasaswiperone"
        versionCode="10" 
        version   = "0.3">


    <name>Picasa Swiper ONE</name>

    <description>
        Picasa Swiper ONE 
    </description>

    <author href="http://szczech.com.pl" email="zs@g.pl">
        ZnakZorro
    </author>
    <preference name="phonegap-version" value="2.2.0" />
